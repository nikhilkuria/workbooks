   "source": [
    "def get_fibonacci_number(position):\n",
    "    if position < 2:\n",
    "        return 1\n",
    "    fib_num = get_fibonacci_number(position-1) + get_fibonacci_number(position-2)\n",
    "    return fib_num"
   ]
  },
